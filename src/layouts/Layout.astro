---
import BlankLayout from "@layouts/BlankLayout.astro";
import Navbar from "@components/theme/navbar/Navbar.astro";
import Footer from "@components/theme/footer/Footer.astro";
import type { SEOType } from "@customTypes/seo/seoType";
import siteInfo from "@data/site-info.json";
//going to revise this as a default layout

const {
  pageTitle,
  pageDescription,
  pageKeywords,
  pageAuthor,
  ogTitle,
  ogType,
  ogImage,
  ogDescription,
} = Astro.props as SEOType;
---

<BlankLayout
  pageTitle={pageTitle}
  pageDescription={pageDescription}
  pageKeywords={pageKeywords}
  pageAuthor={pageAuthor}
  ogTIitle={ogTitle}
  ogType={ogType}
  ogImage={ogImage}
  ogDescription={ogDescription}
>
  <Navbar />
  <slot />
  <Footer />
</BlankLayout>
